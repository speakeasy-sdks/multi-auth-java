  "components": {
    "schemas": {
      "ErrorModel": {
        "title": "ErrorModel",
        "type": "object",
        "required": [
          "message",
          "code"
        ],
        "properties": {
          "message": {
            "type": "string"
          },
          "code": {
            "type": "integer",
            "minimum": 100,
            "maximum": 600
          }
        }
      },
      "Car": {
        "title": "Car",
        "type": "object",
        "required": [
          "name",
          "ownerEmail",
          "numberPlate",
          "previousOwners"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "numberPlate": {
            "type": "string"
          },
          "ownerEmail": {
            "type": "string",
            "format": "email",
            "nullable": false
          },
          "previousOwners": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "securitySchemes": {
      "httpBasic": {
        "type": "http",
        "scheme": "basic"
      },
      "apiKey": {
        "type": "apiKey",
        "in": "query",
        "name": "api_key"
      },
      "apiKeyHeader": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-KEY"
      }
    }
  },
  "security": [
    {
      "apiKey": [],
