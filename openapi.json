  "paths": {
    "/cars/{cnic}": {
      "get": {
        "tags": [
          "cars"
        ],
        "security": [],
        "description": "Returns cars based on CNIC with apiKey and  httpBasic security",
        "summary": "Find cars by CNIC",
        "operationId": "getCarsByCNIC",
        "responses": {
          "200": {
            "description": "car response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Car"
                  }
                }
              }
            }
          },
          "default": {
            "description": "error payload",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "cnic",
          "in": "path",
          "description": "CNIC of a person",
          "required": true,
          "schema": {
            "type": "string"
          },
          "style": "simple"
        }
      ]
    },
    "/carsbyid/{id}": {
      "get": {
        "tags": [
          "cars"
        ],
        "security": [
          {
            "httpBasic": []
          },
          {
            "apiKey": []
          }
        ],
        "description": "Returns cars based on ID with apiKey or httpBasic security",
        "summary": "Find cars by ID",
        "operationId": "getCarsById",
        "responses": {
          "200": {
            "description": "car response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Car"
                  }
                }
              }
            }
          },
          "default": {
            "description": "error payload",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "description": "ID of a person",
          "required": true,
          "schema": {
            "type": "integer"
          },
          "style": "simple"
        }
      ]
    },
    "/carsbyname/{name}": {
      "get": {
        "tags": [
          "cars"
        ],
        "security": [
          {
            "apiKeyHeader": [],
            "httpBasic": []
          }
        ],
        "description": "Returns cars based on NAME with apiKeyHeader and httpBasic security",
        "summary": "Find cars by Name",
        "operationId": "getCarsByName",
        "responses": {
          "200": {
            "description": "car response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Car"
                  }
                }
              }
            }
          },
          "default": {
            "description": "error payload",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
